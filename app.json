{
  "expo": {
    "name": "trackreader",
    "slug": "trackreader",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo.jpeg",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Logo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#1E1E2F"
    },
    "plugins": [
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-8166650997061733~5453313944",
          "iosAppId": "ca-app-pub-8166650997061733~5453313944"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Learn02"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo.jpeg",
        "backgroundColor": "#1E1E2F"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.DOWNLOAD_WITHOUT_NOTIFICATION",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "package": "com.anonymous.Learn02"
    },
    "web": {
      "favicon": "./assets/Logo.jpeg"
    },
    "extra": {
      "eas": {
        "projectId": "03b7af62-48f9-4baa-86b6-9327b538b1a0"
      }
    }
  }
}
